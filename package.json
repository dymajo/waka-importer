  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dymajo/waka-importer/issues"
  },
  "homepage": "https://github.com/dymajo/waka-importer#readme",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/extract-zip": "^1.6.2",
    "@types/mssql": "^4.0.13",
    "@types/node": "^11.13.5",
    "@types/node-fetch": "^2.3.2",
    "@types/request": "^2.48.1",
    "@types/rimraf": "^2.0.2",
    "aws-sdk": "^2.313.0",
    "axios": "^0.18.0",
    "azure-storage": "^2.10.1",
    "colors": "^1.3.1",
    "csv-parse": "^2.5.0",
    "dotenv": "^6.0.0",
    "extract-zip": "^1.6.7",
    "mssql": "^5.1.0",
    "node-fetch": "^2.3.0",
